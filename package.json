  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^14.18.0",
    "@types/pg": "^8.6.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.47.0",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.5",
